<FindBugsFilter>
  <Match>
    <Or>
      <Bug pattern="EI_EXPOSE_REP" />
      <Bug pattern="EI_EXPOSE_REP2" />
    </Or>
    <Or>
      <Package name="~de\.seitenbau\.govdata\..*\.model.*" />
    </Or>
  </Match>
  <Match>
    <Class name="~de\.seitenbau\.govdata\.edit\.gui\.controller\.EditController" />
