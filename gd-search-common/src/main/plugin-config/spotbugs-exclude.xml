<FindBugsFilter>
  <Match>
    <!-- Ignore external files -->
    <Package name="~com\.liferay\.portal\.kernel\.search\.suggest.*" />
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP2" />
    <Class name="de.seitenbau.govdata.search.filter.BoundingBox" />
    <Field name="shapeRelation" />
  </Match>
  <Match>
    <Or>
      <Bug pattern="EI_EXPOSE_REP" />
      <Bug pattern="EI_EXPOSE_REP2" />
    </Or>
    <Or>
      <Class name="~.*\.[a-zA-Z]*Bean.*" />
      <Class name="~.*\.[a-zA-Z]*Container.*" />
      <Class name="de.seitenbau.govdata.search.common.SearchFilterBundle" />
      <Package name="~de\.seitenbau\.govdata\.search\..*\.model.*" />
    </Or>
  </Match>
  <Match>
    <Bug pattern="EI_EXPOSE_REP" />
    <Or>
      <And>
        <Class name="de.seitenbau.govdata.search.common.cache.NumberCache" />
        <Method name="~getRaw(.*)Data" />
      </And>
      <And>
        <Class name="de.seitenbau.govdata.search.common.cache.TweetCache" />
        <Method name="getTweetData" />
      </And>
    </Or>
  </Match>
</FindBugsFilter>
