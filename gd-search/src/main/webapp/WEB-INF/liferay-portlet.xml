    <ajaxable>false</ajaxable>
    <css-class-wrapper>gd-search-atomfeed-portlet</css-class-wrapper>
    <add-default-resource>true</add-default-resource>
  </portlet>
  <portlet>
    <portlet-name>gd-search-details</portlet-name>
